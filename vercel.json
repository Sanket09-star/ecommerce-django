{
    "builds": [
        {
            "src": "greatkart/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "greatkart/wsgi.py"
        }
    ],
    "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "greatkart.settings"
    }
  },
  "commands": {
    "prebuild": "pip install -r requirements.txt",
    "build": "python manage.py collectstatic --noinput && python manage.py migrate"
  }
}